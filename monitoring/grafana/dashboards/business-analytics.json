{
    "id": null,
    "title": "BrainGenTechnology - Business Analytics Dashboard",
    "tags": ["braintech", "business", "analytics", "rag", "leads"],
    "timezone": "browser",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "RAG System Health",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rag_system_up",
            "legendFormat": "System Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Active Conversations (Real-time)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 4, "y": 0},
        "targets": [
          {
            "expr": "rag_active_conversations",
            "legendFormat": "Active Chats"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Total Queries Today",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 0},
        "targets": [
          {
            "expr": "increase(rag_queries_total[24h])",
            "legendFormat": "Queries"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "Lead Conversion Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rag_lead_conversion_rate",
            "legendFormat": "Conversion %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "green", "value": 20}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "RAG Query Response Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(rag_query_duration_seconds_bucket[5m]))",
            "legendFormat": "Median"
          }
        ],
        "yAxes": [
          {
            "unit": "s",
            "min": 0
          }
        ],
        "legend": {
          "show": true
        }
      },
      {
        "id": 6,
        "title": "Query Volume Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 4},
        "targets": [
          {
            "expr": "rate(rag_queries_total[5m]) * 60",
            "legendFormat": "Queries/min"
          }
        ],
        "yAxes": [
          {
            "unit": "reqps",
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "title": "Business Leads by Quality Score",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rag_leads_by_score{score=\"high\"}",
            "legendFormat": "High Quality (80-100)"
          },
          {
            "expr": "rag_leads_by_score{score=\"medium\"}",
            "legendFormat": "Medium Quality (60-79)"
          },
          {
            "expr": "rag_leads_by_score{score=\"low\"}",
            "legendFormat": "Low Quality (0-59)"
          }
        ]
      },
      {
        "id": 8,
        "title": "Lead Generation Trend",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12},
        "targets": [
          {
            "expr": "increase(rag_qualified_leads_total[1h])",
            "legendFormat": "New Leads/hour"
          },
          {
            "expr": "increase(rag_sales_ready_leads_total[1h])",
            "legendFormat": "Sales Ready/hour"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "System Resource Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=\"braintech-rag\"}[5m]) * 100",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "container_memory_usage_bytes{name=\"braintech-rag\"} / container_spec_memory_limit_bytes{name=\"braintech-rag\"} * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 10,
        "title": "Redis Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 20},
        "targets": [
          {
            "expr": "redis_commands_processed_total",
            "legendFormat": "Total Commands"
          },
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0
          }
        ]
      },
      {
        "id": 11,
        "title": "Top User Industries",
        "type": "table",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "topk(10, rag_leads_by_industry)",
            "legendFormat": "{{industry}}"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "industry": "Industry",
                "Value": "Lead Count"
              }
            }
          }
        ]
      },
      {
        "id": 12,
        "title": "User Intent Distribution",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 28},
        "targets": [
          {
            "expr": "rag_user_intent_distribution",
            "legendFormat": "{{intent}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 13,
        "title": "Recent Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 16, "y": 0},
        "targets": [
          {
            "expr": "rate(rag_errors_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "title": "Average Session Duration",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 20, "y": 0},
        "targets": [
          {
            "expr": "rag_avg_session_duration_seconds",
            "legendFormat": "Avg Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      }
    ]
}