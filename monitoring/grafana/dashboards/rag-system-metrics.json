{
    "id": null,
    "title": "RAG System - AI Metrics",
    "tags": ["braintech", "rag", "ai"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Redis Performance Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commands/sec"
          }
        ],
        "yAxes": [{"unit": "s"}],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Cache Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100",
            "legendFormat": "Cache Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Database Keys Count",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_db_keys",
            "legendFormat": "Total Keys"
          }
        ],
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 8}
      },
      {
        "id": 5,
        "title": "Redis Connection Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients"
          },
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commands/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Cache Miss & Expiry Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_keyspace_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          },
          {
            "expr": "rate(redis_expired_keys_total[5m])",
            "legendFormat": "Expired Keys/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "15s"
}